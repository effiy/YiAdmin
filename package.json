{
  "name": "yaai",
  "version": "0.1.1",
  "private": true,
  "description": "YiAdmin 是一款开源的管理系统，基于以下技术栈构建：Vue 3.4、TypeScript、Vite 5、Pinia 和 Element-Plus。",
  "author": {
    "name": "YiAdmin",
    "email": "effiyichengliang@gmail.com",
    "url": "https://github.com/effiy/YiAdmin"
  },
  "scripts": {
    "dev": "vite",
    "doc": "vitepress dev docs --host",
    "test:unit": "vitest --config ./vitest.config.ts",
    "build": "vue-tsc && vite build --mode production",
    "build:dev": "vue-tsc && vite build --mode development",
    "build:test": "vue-tsc && vite build --mode test",
    "commit": "git add -A && czg && git push",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "lint:eslint": "eslint --fix --ext .js,.ts,.vue ./src",
    "lint:lint-staged": "lint-staged",
    "lint:prettier": "prettier --write \"src/**/*.{js,ts,json,tsx,css,less,vue,html,md}\"",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{vue,less,postcss,css}\" --cache --cache-location node_modules/.cache/stylelint/",
    "prepare": "husky install",
    "preview": "pnpm build:dev && vite preview",
    "release": "standard-version",
    "type:check": "vue-tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@vueuse/core": "^10.11.0",
    "@wangeditor/editor": "^5.1.23",
    "@wangeditor/editor-for-vue": "^5.1.12",
    "axios": "^1.7.2",
    "dayjs": "^1.11.11",
    "decimal.js": "^10.4.3",
    "driver.js": "^1.3.1",
    "echarts": "^5.5.1",
    "echarts-liquidfill": "^3.1.0",
    "element-plus": "^2.7.6",
    "md5": "^2.3.0",
    "mitt": "^3.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "qs": "^6.12.1",
    "screenfull": "^6.0.2",
    "sortablejs": "^1.15.2",
    "typed.js": "^2.1.0",
    "vue": "^3.4.31",
    "vue-grid-layout-v3": "^3.1.2",
    "vue-i18n": "^9.13.1",
    "vue-router": "^4.4.0",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@tailwindcss/forms": "^0.5.9",
    "@types/md5": "^2.3.5",
    "@types/nprogress": "^0.2.3",
    "@types/qs": "^6.9.15",
    "@types/sortablejs": "^1.15.8",
    "@types/tailwindcss": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/vue3-jest": "^27.0.0-alpha.1",
    "autoprefixer": "^10.4.20",
    "babel-jest": "^27.0.6",
    "code-inspector-plugin": "^0.16.1",
    "cz-git": "1.9.2",
    "czg": "^1.9.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-tailwindcss": "^3.17.5",
    "eslint-plugin-vue": "^9.26.0",
    "husky": "^9.0.11",
    "jest": "^27.0.5",
    "less": "^4.2.1",
    "lint-staged": "^15.2.5",
    "markdown-it-custom-attrs": "^1.0.2",
    "postcss": "^8.4.49",
    "postcss-html": "^1.7.0",
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "rollup-plugin-visualizer": "^5.12.0",
    "standard-version": "^9.5.0",
    "stylelint": "^16.6.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recess-order": "^5.0.1",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-less": "^3.0.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.5.2",
    "unplugin-vue-components": "^0.28.0",
    "unplugin-vue-setup-extend-plus": "^1.0.1",
    "vite": "^5.3.2",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-pwa": "^0.20.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-tailwindcss": "^0.0.0-0",
    "vite-plugin-vue-devtools": "^7.3.5",
    "vitepress": "^1.5.0",
    "vitest": "^2.1.8",
    "vue-tsc": "^2.0.22"
  },
  "browserslist": {
    "production": [
      "> 1%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "bugs": {
    "url": "https://github.com/effiy/YiAdmin/issues"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "engines": {
    "node": ">=16.18.0"
  },
  "homepage": "https://github.com/effiy/YiAdmin",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:effie/YiAdmin.git"
  }
}
